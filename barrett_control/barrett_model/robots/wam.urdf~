  # velocity halved
  <joint name="wam_j1_joint" type="revolute">
     <origin xyz="0.22 0.14 0.346" rpy="-1.57079633 0 0" /> 
     <parent link="wam_base"/>
     <child link="j1" />
     <axis xyz="0 -1 0"/>
     <limit effort="30" velocity="1.5" lower="-2.61" upper="2.61"/>
