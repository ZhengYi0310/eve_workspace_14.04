<launch>
	<arg name="model" />
	<arg name="gui" default="True" />
	
    <!-- Launchfile Arguments (optional)-->
    <arg name="robot_name" default="wam"/>
    <param name="root_link" value="base_link"/>
    <param name="tip_link" value="j7"/>
    <param name="tip_joint" value="$(arg robot_name)/palm_yaw_joint"/>

  <!-- Component Configuration -->
    
	<param name="robot_description_yi" textfile="$(find barrett_model)/robots/wam.urdf" />
	<param name="use_gui" value="$(arg gui)"/>
	
</launch>
